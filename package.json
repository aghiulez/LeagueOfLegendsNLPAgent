{
  "name": "league-of-legends-agent-fulfillment",
  "version": "1.0.0",
  "description": "Dialogflow Agent Intent Fulfillment Source - following  an architecture that allows local development (via ngrok) and streamlined deployment to Google Cloud Functions for production.",
  "main": "src/cloudFuncs.js",
  "engines": {
    "node": "10"
  },
  "scripts": {
    "dev": "nodemon --inspect src/server.js",
    "start": "node src/server.js",
    "tunnel": "ngrok http 8080",
    "lint": "standard src/**/*.js",
    "deploy-cf": "firebase deploy --only functions",
    "deploy-ae": "gcloud app deploy"
  },
  "dependencies": {
    "express": "^4.16.4",
    "actions-on-google": "^2.2.0",
    "firebase-admin": "^9.5.0",
    "firebase-functions": "^3.13.2",
    "dialogflow": "^0.6.0",
    "dialogflow-fulfillment": "^0.5.0"
  },
  "devDependencies": {
    "ngrok": "^3.4.1",
    "nodemon": "^2.0.7"
  },
  "author": "Ara Matthew",
  "license": "ISC"
}
